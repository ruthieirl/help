{
  "name": "Help",
  "version": "1.0.0",
  "description": "Help! is a web based application that provides quick and easy access to Autobody and Repair Shops, Towing Companies, and Emergency resources in your area. Help! is built with the MERN stack ( MongoDB, Express, React, Node.js).",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrainaud/help.git"
  },
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "client": "node scripts/start-client.js",
    "start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "build": "node scripts/build.js",
    "seed": "node scripts/seedDB.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ruthie Campiz & William Rainaud",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-beta.3",
    "cloudinary": "^1.9.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "jquery-file-upload-middleware": "^0.1.8",
    "mongoose": "^4.13.7",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-alpha.4"
